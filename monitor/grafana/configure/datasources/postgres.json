{
  "uid": "postgres",
  "orgId": 1,
  "name": "PostgreSQL",
  "type": "postgres",
  "typeName": "PostgreSQL",
  "typeLogoUrl": "public/app/plugins/datasource/postgres/img/postgresql_logo.svg",
  "access": "proxy",
  "url": "${PG_URL}",
  "user": "${PG_USER}",
  "database": "${PG_DATABASE}",
  "basicAuth": false,
  "isDefault": false,
  "jsonData": {
    "postgresVersion": 903,
    "sslmode": "disable",
    "tlsAuth": false,
    "tlsAuthWithCACert": false,
    "tlsConfigurationMethod": "file-path",
    "tlsSkipVerify": true
  },
  "secureJsonData": {
    "password": "${PG_PASSWORD}"
  },
  "readOnly": false
}
